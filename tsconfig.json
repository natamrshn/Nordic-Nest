{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"~assets/*": [
				"src/assets/*"
			],
			"~modules/*": [
				"src/modules/*"
			],
			"~router/*": [
				"src/router/*"
			],
			"~shared/*": [
				"src/shared/*"
			],
			"~types/*": [
				"src/types/*"
			],
			"~/*": [
				"src/*"
			],
			"~ai/*": [
				"src/modules/ai/*"
			]
		}
	}
}