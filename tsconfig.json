{
	"compilerOptions": {
		"esModuleInterop": true,
		"baseUrl": ".",
		"jsx": "react-jsx",
		"paths": {
			"~assets/*": [
				"src/assets/*"
			],
			"~modules/*": [
				"src/modules/*"
			],
			"~router/*": [
				"src/router/*"
			],
			"~shared/*": [
				"src/shared/*"
			],
			"~store/*": [
				"src/store/*"
			],
			"~typings/*": [
				"src/typings/*"
			],
			"~/*": [
				"src/*"
			],
			"~types/*": [
				"src/types/*"
			],
			"~stores/*": [
				"src/shared/stores/*"
			],
			"~about-us/*": [
				"src/modules/about-us/*"
			]
		}
	}
}