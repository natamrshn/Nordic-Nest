{
	"compilerOptions": {
	  "esModuleInterop": true,
	  "baseUrl": ".",
	  "jsx": "react-jsx",
	  "paths": {
		"~assets/*": ["src/assets/*"],
		"~modules/*": ["src/modules/*"],
		"~router/*": ["src/router/*"],
		"~shared/*": ["src/shared/*"],
		"~/*": ["src/*"],
		"~types/*": ["src/types/*"],
		"~kitchen/*": ["src/modules/kitchen/*"],
		"~constants/*": ["src/shared/constants/*"]
	  }
	}
  }
  