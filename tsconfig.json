{
	"compilerOptions": {
		"esModuleInterop": true,
		"lib": [
			"ES2015",
			"DOM"
		],
		"baseUrl": ".",
		"jsx": "react-jsx",
		"paths": {
			"~assets/*": [
				"src/assets/*"
			],
			"~modules/*": [
				"src/modules/*"
			],
			"~router/*": [
				"src/router/*"
			],
			"~shared/*": [
				"src/shared/*"
			],
			"~/*": [
				"src/*"
			],
			"~types/*": [
				"src/types/*"
			],
			"~stores/*": [
				"src/shared/stores/*"
			],
			"~about-us/*": [
				"src/modules/about-us/*"
			],
			"~ai/*": [
				"src/modules/ai/*"
			],
			"~livingRoom – копія/*": [
				"src/modules/livingRoom – копія/*"
			]
		}
	}
}